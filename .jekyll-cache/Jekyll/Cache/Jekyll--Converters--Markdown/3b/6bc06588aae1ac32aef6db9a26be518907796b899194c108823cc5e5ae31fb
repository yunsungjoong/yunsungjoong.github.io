<div class="language-plaintext highlighter-rouge"><div class="highlight"><pre class="highlight"><code>var player = {
  hp: 100,
  mp: 300,
  hit: function (damage) {
    this.hp -= damage;
    console.log('player hit damage : ' + damage);
    return this.isDie();
  },
  isDie: function() {
    if (this.hp &lt;= 0) {
      console.log('player is die...');
      return true:
    }
    return false;
  }
};

player.hit(150);
